{
  "env": {
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "@104corp/eslint-config-104z/vue3"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-v-html": "off"
  }
}
